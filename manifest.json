{
    "name" : "VULMS Enhancer",
    "description" : "VULMS Enhancer beautifys the VULMS interface. It replaces the student's name, profile image and instructors' profile image.",
    "version" : "1.0.0",
    "author" : "Muhammad Gulzaib",
    "manifest_version" : 3,
    "content_scripts" : [
        {
            "matches" : ["https://vulms.vu.edu.pk/*"],
            "js" : ["js/jquery-3.6.0.min.js", "js/vulms_privacy_hider.js"]
        }
    ],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : {
            "16" : "/images/get_started16.png",
            "32" : "/images/get_started32.png",
            "48" : "/images/get_started48.png",
            "128" : "/images/get_started128.png"
        }
    },
    "icons" : {
        "16" : "/images/get_started16.png",
        "32" : "/images/get_started32.png",
        "48" : "/images/get_started48.png",
        "128" : "/images/get_started128.png"
    }
}